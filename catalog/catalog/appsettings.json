{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jaeger": {
    "AgentHost": "jaeger",
    "AgentPort": 6831,
